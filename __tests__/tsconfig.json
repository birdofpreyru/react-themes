{
  "extends": "@tsconfig/recommended",
  "exclude": [
    "ts-types/"
  ],
  "include": [".."],
  "compilerOptions": {
    "jsx": "react-jsx",
    "noEmit": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "rootDirs": [
      "../build/scss-types/docs",
      "../build/scss-types/jest",
      "../docs",
      "../jest"
    ],
    "strict": true,
    "verbatimModuleSyntax": true
  }
}
