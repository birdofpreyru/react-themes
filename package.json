{
  "name": "@dr.pogodin/react-themes",
  "version": "1.4.7",
  "description": "UI theme composition with CSS Modules and React",
  "main": "./build/node/index.js",
  "exports": {
    "module": "./build/module/index.js",
    "node": "./build/node/index.js",
    "default": "./build/node/index.js"
  },
  "scripts": {
    "build": "rimraf build && npm run build:module && npm run build:node",
    "build:module": "rimraf build/module && babel src --out-dir build/module --source-maps --config-file ./babel-module.config.js",
    "build:node": "rimraf build/node && babel src --out-dir build/node --source-maps",
    "jest": "jest --config jest/config.json --no-cache",
    "lint": "eslint --ext .js,.jsx .",
    "test": "npm run lint && npm run jest"
  },
  "repository": "github:birdofpreyru/react-themes",
  "keywords": [
    "React",
    "ReactJS",
    "JS",
    "JavaSript",
    "UI",
    "theme",
    "CSS",
    "Modules",
    "Frontend"
  ],
  "author": "Dr. Sergey Pogodin <doc@pogodin.studio> (https://dr.pogodin.studio)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/birdofpreyru/react-themes/issues"
  },
  "homepage": "https://dr.pogodin.studio/docs/react-themes/index.html",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@dr.pogodin/babel-plugin-react-css-modules": "^6.9.2",
    "babel-jest": "^29.3.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.27.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.3.1",
    "postcss": "^8.4.19",
    "postcss-scss": "^4.0.5",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.1"
  }
}
