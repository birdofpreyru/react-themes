{
  "name": "@dr.pogodin/react-themes",
  "version": "1.3.0",
  "description": "UI theme composition with CSS Modules and React",
  "main": "index.js",
  "scripts": {
    "build": "rimraf build && npm run build:node && npm run build:web",
    "build:node": "rimraf build/node && babel src --out-dir build/node --source-maps",
    "build:web": "rimraf build/web && babel src --out-dir build/web --source-maps --config-file ./babel.web.config.js",
    "jest": "jest --config jest/config.json",
    "lint": "eslint --ext .js,.jsx .",
    "test": "npm run lint && npm run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/birdofpreyru/react-themes.git"
  },
  "keywords": [
    "React",
    "ReactJS",
    "JS",
    "JavaSript",
    "UI",
    "theme",
    "CSS",
    "Modules",
    "Frontend"
  ],
  "author": "Dr. Sergey Pogodin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/birdofpreyru/react-themes/issues"
  },
  "homepage": "https://dr.pogodin.studio",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@dr.pogodin/babel-plugin-css-modules-transform": "^1.8.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "jest": "^26.5.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-test-renderer": "^16.14.0",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "prop-types": "^15.7.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.1"
  }
}
